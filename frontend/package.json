{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-git-hooks": "cd .. && node .secret-scan/secret-scan.js -- --check-git-hooks",
    "format": "bun run check-git-hooks && prettier --write .",
    "lint-fix": "bun run check-git-hooks && (eslint --fix --cache --report-unused-disable-directives . || true) && prettier --write .",
    "lint-check": "bun run check-git-hooks && eslint --cache --report-unused-disable-directives . && prettier --check .",
    "prepare": "cd .. && husky install .husky"
  },
  "dependencies": {
    "@types/node": "^20.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.21.0",
    "styled-components": "^6.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
